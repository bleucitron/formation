{
  "name": "formation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "tweets": "node -r esm generateTweets.js",
    "build": "browserify src/vanilla/main.js -o bundle.js -t [ babelify ] -d",
    "watch": "watchify src/vanilla/main.js -o bundle.js -t [ babelify ] -d -v",
    "build:ts": "browserify src/typescript/main.ts -o bundle.js -p [ tsify ] -t [ babelify ] -d --extension=ts",
    "watch:ts": "watchify src/typescript/main.ts -o bundle.js -p [ tsify ] -t [ babelify ] -d -v --extension=ts",
    "build:react": "browserify src/react/main.jsx -o bundle.js -t [ babelify ] -d --extension=jsx",
    "watch:react": "watchify src/react/main.jsx -o bundle.js -t [ babelify ] -d -v --extension=jsx",
    "build:redux": "browserify src/redux/main.jsx -o bundle.js -t [ babelify ] -d --extension=jsx",
    "watch:redux": "watchify src/redux/main.jsx -o bundle.js -t [ babelify ] -d -v --extension=jsx"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --findRelatedTests"
    ]
  },
  "huksy": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@types/classnames": "^2.2.9",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "husky": "^1.3.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^25.0.0",
    "lint-staged": "^7.3.0",
    "tsify": "^4.0.1",
    "typescript": "^3.7.5",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "twit": "^2.2.11"
  }
}
